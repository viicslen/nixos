{
    "dark.primary": "#51a4e7",
    "dark.error": "#e55f86",
    "dark.success": "#00D787",
    "dark.bg": "#171717",
    "dark.fg": "#eeeeee",
    "dark.widget": "#eeeeee",
    "dark.border": "#eeeeee",
    "light.primary": "#426ede",
